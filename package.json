{
    "name": "coding-challenge-frontend",
    "version": "1.0.0",
    "description": "coding challenge ",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "setup": "cd backend && npm install && cd ..  && cd frontend && npm install",
        "check": "cd backend && npx prettier --check . && cd ..  && cd frontend && npx prettier --check .",
        "format": "cd backend && npx prettier --write . && cd ..  && cd frontend && npx prettier --write .",
        "server": "cd backend && node index.js",
        "client": "cd frontend && npm start",
        "build": "cd frontend && npm run build",
        "storybook": "cd frontend && npm run storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Legal-One/coding-challenge-frontend.git"
    },
